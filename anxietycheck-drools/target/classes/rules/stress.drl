package com.main

import com.ftn.sbnz.anxietycheck.model.TestTakingUser;

rule "Medium stress level"
	no-loop
		lock-on-active true
    when
        $t: TestTakingUser(  $t.getStressPoints()/40*100 < 65 && $t.getStressPoints()/40*100 > 30 )
    then
    	System.out.println("Srednja kolicina stresa" );  
    	$t.setStress(StressCategory.MEDIUM)
    	update($t)    
end
